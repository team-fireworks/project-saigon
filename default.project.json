{
    "name": "Total Fire Towers/Project Saigon",
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json"
    ],
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "tftServer": {
                "$path": "out/server"
            }
        },
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "rbxts_include": {
                "$path": "include",
                "node_modules": {
                    "$className": "Folder",
                    "@rbxts": {
                        "$path": "node_modules/@rbxts"
                    },
                    "@flamework": {
                        "$path": "node_modules/@flamework"
                    }
                }
            },
            "tftClient": {
                "$path": "out/client"
            },
            "tftShared": {
                "$path": "out/shared"
            },
            "Mechanics": {
                "$path": "luau/mechanics"
            },
            "Types": {
                "$path": "luau/types.luau"
            },
            "isServerIgnited": {
                "$className": "BoolValue",
                "$properties": {
                    "Value": false
                }
            }
        }
    }
}
